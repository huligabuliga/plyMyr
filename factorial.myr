program factorial;
vars 
    int num;

function int factorial(int n)
vars
    int result;
{
    if (n == 0) then {
        return (1);
    } 
    else {
        write("iteration factorial:", n);
        return (n * factorial(n - 1));
    }
}
   
main(){
  num = 5;
  factorial(num);
  write("factorial is", num);
}