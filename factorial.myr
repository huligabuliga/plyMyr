program factorial;
vars 
    int num;

function int factorial(int n)
vars
    int result;
{
    if (n == 0) then {
        return (1);
        write("factorial:", n);
        
    } 
    else {
        return (n * factorial(n - 1));
        write("factorial:", n);
    }
}
   
main(){
  num = 5;
  factorial(num);
}